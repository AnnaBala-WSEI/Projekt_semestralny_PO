<Window x:Class="Projekt_semestralny_PO.GamesWindow"
        xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
        xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
        xmlns:d="http://schemas.microsoft.com/expression/blend/2008"
        xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006"
        xmlns:local="clr-namespace:Projekt_semestralny_PO"
        mc:Ignorable="d"
        Title="GamesWindow" Height="450" Width="800">
    <Grid>
        <Grid.ColumnDefinitions>
            <ColumnDefinition Width="20*"/>
            <ColumnDefinition Width="79*"/>
        </Grid.ColumnDefinitions>
        <TextBlock TextAlignment="Center" HorizontalAlignment="Left" Margin="10,10,0,0" TextWrapping="Wrap" Text="Video games list" VerticalAlignment="Top" Height="28" Width="772" Grid.ColumnSpan="2"/>
        <DataGrid x:Name="gridVideoGames" HorizontalAlignment="Left" Height="190" Margin="10,43,0,0" VerticalAlignment="Top" Width="772" RenderTransformOrigin="-3.66,-0.94" Grid.ColumnSpan="2"/>

    </Grid>
</Window>
